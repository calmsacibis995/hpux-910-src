/usr/etc/yp/ypxfr protocols.bynumber
/usr/etc/yp/ypxfr rpc.bynumber
/usr/etc/yp/ypxfr services.byname
/usr/etc/yp/ypxfr ypservers
/usr/etc/yp/ypxfr vhe_list
