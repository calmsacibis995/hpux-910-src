/* The new-style definitions here assume __STDC__ */
#include <sys/stdsyms.h>
#ifndef __STDC__
#define __STDC__
#endif

/* HP-UX entry points */
/* =========================  HPUX ============================= */
#ifdef _INCLUDE_HPUX_SOURCE

#include <dbm.h>
