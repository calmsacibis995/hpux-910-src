default:	true.sh
	 /bin/cp true.sh true
clean:
	rm -f *.o *.msg

clobber:	clean
	rm -f true.sh

# copy /bin/true to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/true
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.

install:	true
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! cp true ../../../Filesets.sl/UX-CORE/bin
	! chmog 0555 bin bin ../../../Filesets.sl/UX-CORE/bin/true
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
# copy /bin/true to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/true
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! cp true ../../../Filesets.sl/UX-CORE/bin/true
	! chmog 0555 bin bin ../../../Filesets.sl/UX-CORE/bin/true
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/true to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/true
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/true ../../../Filesets.sl/UX-CORE/bin/hp-mc680x0
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/true to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/true
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/true ../../../Filesets.sl/UX-CORE/bin/hp9000s200
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/true to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/true
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/true ../../../Filesets.sl/UX-CORE/bin/hp9000s300
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/true to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/true
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/true ../../../Filesets.sl/UX-CORE/bin/hp9000s400
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
