default:	ls.c
	 $(CC) -Wl,-a,archive ls.c -o ls /usr/lib/libcurses.a
	 findmsg ls.c > ls.msg
	 gencat ls.cat ls.msg
clean:
	rm -f *.o *.msg

clobber:	clean
	rm -f ls

# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.

install:	ls
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! cp ls ../../../Filesets.sl/UX-CORE/bin
	! chmog 0555 bin bin ../../../Filesets.sl/UX-CORE/bin/ls
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! cp ls ../../../Filesets.sl/UX-CORE/bin/ls
	! chmog 0555 bin bin ../../../Filesets.sl/UX-CORE/bin/ls
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0444, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/usr/lib/nls/C
	! cp ls.cat ../../../Filesets.sl/UX-CORE/usr/lib/nls/C/ls.cat
	! chmog 0444 bin bin ../../../Filesets.sl/UX-CORE/usr/lib/nls/C/ls.cat
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/usr/lib/nls/C
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/usr/lib/nls/
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/usr/lib/
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/usr/
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/ls ../../../Filesets.sl/UX-CORE/bin/l
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/ls ../../../Filesets.sl/UX-CORE/bin/ll
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/ls ../../../Filesets.sl/UX-CORE/bin/lsf
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/ls ../../../Filesets.sl/UX-CORE/bin/lsr
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
	! chmog 555 bin bin ../../../Filesets.sl/
# copy /bin/ls to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/ls
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! ln ../../../Filesets.sl/UX-CORE/bin/ls ../../../Filesets.sl/UX-CORE/bin/lsx
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
