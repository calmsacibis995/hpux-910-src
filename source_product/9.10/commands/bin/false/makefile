default:	false.sh
	 /bin/cp false.sh false
clean:
	rm -f *.o *.msg

clobber:	clean
	rm -f false.sh

# copy /bin/false to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/false
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.

install:	false
	mkdir -p ../../../Filesets.sl/UX-CORE/bin
	! cp false ../../../Filesets.sl/UX-CORE/bin
	! chmog 0555 bin bin ../../../Filesets.sl/UX-CORE/bin/false
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/bin
	! chmog 555 bin bin ../../../Filesets.sl/UX-CORE/
# copy /bin/false to the . relative fileset UX-CORE
# change mode, owner, and group on /bin/false
# to 0555, bin, and bin
# change mode, owner, and group on all the parent
# directories.
